list user {
	key name;
	unique uid;
	leaf name {
		type string;
	}
	leaf uid {
		type uint32;
	}
	leaf full-name{
		type string;
	}
	lead class {
		type string;
		default viewer;
	}
}


/user{yang}/name = yang
/user{yang}/uid = 1010



list route {
	key "ip prefix"
}

